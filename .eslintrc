{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", { "trailingComma": "all" }],
    "no-console": ["warn", { "allow": ["warn", "error", "info"]  }]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
